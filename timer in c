#include <time.h>
struct timespec start, end;                  // Define variables

clock_gettime(CLOCK_MONOTONIC, &start);      // Record start time

// Main Function

clock_gettime(CLOCK_MONOTONIC, &end);        // Record end time

double elapsed = (end.tv_sec - start.tv_sec) + (end.tv_nsec - start.tv_nsec) / 1e9; // Calculate time elapsed
printf("Elapsed time: %f seconds\n", elapsed);                                      // Print time elapsed
